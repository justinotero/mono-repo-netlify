{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "first",
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "tslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "css",
      "linter": "tslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "css",
      "linter": "tslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    }
  },
  "projects": {
    "first": {
      "projectType": "application",
      "root": "apps/first",
      "sourceRoot": "apps/first/src",
      "prefix": "test-mono",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/first",
            "index": "apps/first/src/index.html",
            "main": "apps/first/src/main.ts",
            "polyfills": "apps/first/src/polyfills.ts",
            "tsConfig": "apps/first/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/first/src/favicon.ico", "apps/first/src/assets"],
            "styles": ["apps/first/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/first/src/environments/environment.ts",
                  "with": "apps/first/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "first:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "first:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "first:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/first/tsconfig.app.json",
              "apps/first/tsconfig.spec.json",
              "apps/first/tsconfig.editor.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/first/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/first"],
          "options": {
            "jestConfig": "apps/first/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "first-e2e": {
      "root": "apps/first-e2e",
      "sourceRoot": "apps/first-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/first-e2e/cypress.json",
            "tsConfig": "apps/first-e2e/tsconfig.e2e.json",
            "devServerTarget": "first:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "first:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/first-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/first-e2e/**/*"]
          }
        }
      }
    },
    "second-app": {
      "projectType": "application",
      "root": "apps/second-app",
      "sourceRoot": "apps/second-app/src",
      "prefix": "test-mono",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/second-app",
            "index": "apps/second-app/src/index.html",
            "main": "apps/second-app/src/main.ts",
            "polyfills": "apps/second-app/src/polyfills.ts",
            "tsConfig": "apps/second-app/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/second-app/src/favicon.ico",
              "apps/second-app/src/assets"
            ],
            "styles": ["apps/second-app/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/second-app/src/environments/environment.ts",
                  "with": "apps/second-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "second-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "second-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "second-app:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/second-app/tsconfig.app.json",
              "apps/second-app/tsconfig.spec.json",
              "apps/second-app/tsconfig.editor.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/second-app/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/second-app"],
          "options": {
            "jestConfig": "apps/second-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "second-app-e2e": {
      "root": "apps/second-app-e2e",
      "sourceRoot": "apps/second-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/second-app-e2e/cypress.json",
            "tsConfig": "apps/second-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "second-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "second-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/second-app-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/second-app-e2e/**/*"]
          }
        }
      }
    }
  }
}
