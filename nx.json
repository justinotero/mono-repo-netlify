  },
  "projects": {
    "first": { "tags": [] },
    "first-e2e": { "tags": [], "implicitDependencies": ["first"] },
    "second-app": { "tags": [] },
    "second-app-e2e": { "tags": [], "implicitDependencies": ["second-app"] }
  }
}
